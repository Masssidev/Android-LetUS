        <activity android:name=".LikeBoxActivity" />
        <activity android:name=".ResultStudyActivity" />
        <activity android:name=".ResultNoticeActivity" />
        <activity android:name=".MyArticleActivity" />
        <activity android:name=".EditMyStudyActivity" />
