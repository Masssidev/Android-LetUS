        <activity android:name=".SentMessageActivity" />
        <activity android:name=".ReceivedMessageActivity" />
        <activity android:name=".CreateNoticeActivity" />
        <activity android:name=".CreateStudyActivity" />
        <activity android:name=".SelectedFairActivity" />
